<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Example customization of dimensions originally defined in res/values/dimen.xml
         (such as screen margins) for screens with more than 820dp of available width. This
         would include 7" and 10" devices in landscape (~960dp and ~1280dp respectively). -->
    <dimen name="activity_horizontal_margin">64dp</dimen>
    <dimen name="design_fab_size_normal" tools:ignore="PrivateResource">75dp</dimen>
    <dimen name="design_fab_size_mini" tools:ignore="PrivateResource">60dp</dimen>
</resources>
